class WxpayConfig
  include  BaseModel
  
  field :name, type:String, default:"微信手机"
  field :desc, type:String, default:"微信手机"

  field :is_open, type:Boolean, default:false #on off

  field :app_id, type:String
  field :mch_id, type:String  #微信支付商户号, 微信支付分配的商户收款账号
  field :api_key, type:String 
  field :app_secret, type:String  #APPID对应的接口密码，用于获取接口调用凭证access_token时使用  
end
