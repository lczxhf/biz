.ico-del {width: 35px;height: 35px;}

.btnC { position: fixed; left: 0; bottom: 0;  width: 100%;  padding: 8px 0;  border-top: 1px solid #e5e5e5; background: #eaedf1;text-align: right;}
.btnC p {position: absolute;  padding-left: 10px; top: 0px;  font-size: 14px;line-height: 52px;color: #999999;}
.btnC a { display: block; float: right; margin-right: 10px; padding: 0 25px;}
.btnC p .txt-red{padding: 0 5px;}
.btnRed-t { display: inline-block; font-size: 14px; color: #444;border: 1px solid #ccc; background: #db3652; line-height: 34px; padding: 0 10px; -moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius: 5px;}
.btnRed-t.hover { color: #fff; background: #db3652; border: 1px solid #db3652;-moz-border-radius: 3px; -webkit-border-radius: 3px;border-radius: 3px;}

/*夺宝记录*/
.award-record{background: #fff;}
.award-record li { padding: 10px;display: inline-block;border-bottom: 1px solid #E6E6E6;width: 100%}
.award-record li .fl { width: 15%; margin-right: 3%;}
.award-record li .fr{width: 82%;position: relative;}
.award-record li .fl img{border-radius: 50%;}
.award-record li h2{color:#3c3c3c;font-size:16px;margin-bottom:8px;overflow: hidden; text-overflow: ellipsis;display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;line-height: 25px;}
.award-record li p{line-height: 25px;font-size: 14px;color:#999999; overflow: hidden;text-overflow: ellipsis;display: -webkit-box; -webkit-line-clamp: 1;-webkit-box-orient: vertical;}
.award-record li p span{color: #db3652;}
.award-record li p a{color: #519fff;}
.award-record li .look{padding-left: 10px;color: #519fff;}
.award-record-h{min-height: 0;}
.noMore{text-align: center;background: #eee;line-height: 50px;color: #999999;font-size: 14px;}

.message{margin: 0 4%;background: #fff;border: 1px solid #e6e6e6;border-radius: 5px;position: absolute;top:46%;z-index: 999;}
.message-pdd{padding:16px;}
.message h2{color: #3c3c3c;font-size: 16px;}
.message p{line-height: 30px;font-size: 14px;color: #999999;}
.message p span{color: #db3652;font-weight: bold;}
.message ul{width: 100%;display: inline-block;}
.message ul li{float: left;width: 26%;line-height: 20px;color: #999999;font-size: 14px ;}
.confirm{text-align: center;border-top: 1px solid #E6E6E6;line-height: 45px;color: #519fff;font-weight: bold;font-size: 16px;}
.black_overlay { display: none; position: absolute; top: 0%; left: 0%; width: 100%; height: 100%; background:#000;z-index: 0; -moz-opacity: 0.5; opacity: .50;}

.snatching{text-align: center;color: #999999;font-size: 14px;background: #eee;line-height: 25px;padding-top: 15%;}

.nav_index-cart{ position:absolute;right:10px;top:2px;width:48px;}
.nav_index-cart a{display:inline-block;width:100%;height:44px;}
.nav_index-cart a img{width:24px;margin-top:10px;margin-left:24px;}

/*index*/
.catalogue-T2 { height: 30px;  padding: 10px 0; background: #eee;}
.catalogue-T2 > h3 { width: 92%; height: 25px;line-height: 25px;margin: 0 auto; border-left: 8px solid #db3652; box-sizing: border-box;text-indent: 12px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; font-size: 18px; color: #333; font-family: "微软雅黑",san-serif; font-weight: normal;}
.w-goodsList{background: #fff;}
.w-goodsList ul{}
.w-goodsList ul li{padding:10px;border-bottom: 1px solid #eee;position: relative;}
.w-goodsList ul li .w-goodsList-l{width: 90px;height:90px;float: left;}
.w-goodsList ul li .w-goodsList-l a{display: inline-block;}
.w-goodsList ul li .w-goodsList-r{margin-left: 100px;}
.w-goodsList ul li h2{font-size: 16px; height:48px;overflow: hidden;text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;line-height: 25px;  color: #3c3c3c; margin-bottom: 8px;}
.w-goodsList li .wrap {  position: relative; height: 8px; border-radius: 5px; background-color: #efeeee;  overflow: hidden;}
.w-goodsList li .wrap .bar, .w-goodsList li .wrap .color{ display: block; height: 100%;}
.w-goodsList li .wrap .color {  width: 100%; background: #FF8533;background: -webkit-gradient(linear,left top,right top,from(#FFCB3D),to(#FF8533)); background: -moz-linear-gradient(left,#FFCB3D,#FF8533); background: -o-linear-gradient(left,#FFCB3D,#FF8533);  background: -ms-linear-gradient(left,#FFCB3D,#FF8533);}
.w-goodsList li .w-goods-shortFunc { position: absolute; right: 10px; bottom: 20px;}

.w-button-round { display: block; padding: 0; width: 32px;height: 32px; background: #db3652;border: 0; border-radius: 50%; text-indent: -999em; overflow: hidden; position: relative;}
.w-button-addToCart:before {top: 15px; left: 8px; height: 0; border-top: 2px solid #fff;}
.w-button-addToCart:after { top: 8px;  left: 15px; width: 0; border-left: 2px solid #fff;}
.w-button-addToCart:before, .w-button-addToCart:after { content: ""; display: block; position: absolute; width: 17px; height: 17px; overflow: hidden;}
.goodsList-mt{margin-right: 50px;}
.load-over{text-align: center;line-height: 35px;color: #999999;}

/*计算公式*/
.formula{background: #db3652;color: #fff;border-radius: 5px;font-size: 14px;padding: 10px;}
.formula h2,.formula p{font-size: 12px;line-height: 25px;}
.formula-pdd{padding: 4% 2%;}
.numA{padding:3% 2%;border: 1px solid #eee;border-right: 0;border-left: 0;font-size: 12px;line-height: 25px;background: #fff;}
.numA h2{color: #3c3c3c;font-size: 14px;}
.numA p{color: #999999;}
.numA p strong{color: #db3652;font-weight: bold;}
.numB{padding:3% 2%;border-bottom: 1px solid #eee;font-size: 12px;line-height: 25px; background: #fff;}
.numB h2{color: #3c3c3c;font-size: 14px;}
.numB p{color: #999999;}
.numB .txt-blue{padding-left: 10px;}
.conut-result{padding:3% 2%;margin-top:10px;border-bottom: 1px solid #eee;font-size: 12px;line-height: 25px; background: #fff;color: #3c3c3c;}
.conut-result h2{font-size: 14px;}
.conut-result div{text-align: center;line-height: 30px;}
.conut-result span{padding-left: 10px;}
.spread{}
.spread p{float: left;width: 80%;}
.spread .show{float:right;width: 20%;text-align: right;color: #519fff;}

.count-message{background: #fff;}
.m-calc-resultList { width: 100%; border-bottom: 1px solid #eee;color: #999999;}
.m-calc-resultList th{color: #999999;font-weight: normal;}
.m-calc-resultList th { background: #f8f8f8; text-align: left;}
.m-calc-resultList th, .m-calc-resultList td { padding-left: 10px; line-height: 22px;  border-bottom: 1px solid #eee;}
.ico-arrow-transfer { width: 10px;  height: 6px; position: relative; background: none;}
.ico-arrow-transfer:before {  left: 0; top: 2px; border-top: 2px solid #db3625; width: 5px; height: 0;}
.ico-arrow-transfer:after { top: 1px; right: 0; width: 0;height: 0; border-width: 4px;  border-top: 0; border-style: solid; border-color: transparent; border-bottom-color: #db3625;  transform: rotate(90deg); -webkit-transform: rotate(90deg); -moz-transform: rotate(90deg);}
.ico-arrow-transfer:before, .ico-arrow-transfer:after { content: ""; position: absolute; display: block;}
.m-calc-resultList td.user a { color: #999999;}
.m-calc-resultList td.time{white-space:nowrap;}



/*支付订单*/
.pay-order{line-height: 25px;color: #999999;font-size: 14px;background: #fff;}
.pay-order ul{border-bottom:1px solid #e1e1e1;padding-bottom: 10px;}
.pay-order ul li{padding: 10px 10px 0;position: relative;}
.pay-order ul li span{position: absolute;right: 10px;bottom: 0;}
.pay-order ul li span em{color: #db3652;font-style:normal;font-weight: bold;}
.pay-order ul li .pay-order-l{margin-right: 40px;}
.goods-together{padding:0 10px;text-align: right;line-height: 35px;border-bottom:1px solid #e1e1e1;}
.goods-together span{color: #db3652;font-weight: bold;margin-left:10px ;}
.goPay{position: fixed;bottom: 0;width: 92%;margin:4%;text-align: center;line-height: 37px;color: #fff;background: #db3652;border-radius: 5px;font-size: 16px;}

/*等待支付结果*/
.wait-pay-result{position:relative;text-align:center;border-bottom: 1px solid #e1e1e1;background: #fff;color: #999999;padding-bottom: 8%;}
.wait-pay-result div{padding: 8% 0;}
.wait-pay-result h2{width:34%;margin:0 auto;background: url(/assets/images/ico_25a.png) no-repeat left center;background-size: 18px 18px;color: #333; }
.wait-pay-result p{line-height: 25px;font-size: 14px;}
.wait-pay-result p span{color: #db3652;padding-left: 10px;}
.balance{margin-top: 8px;background: #fff;font-size: 14px;}
.input-balance{border: 1px solid #ddd;border-right:0;border-left:0;width: 100%;text-indent: 2%;line-height: 40px;}

.balance .balance-1{width: 96%;padding: 0 2%;border-bottom:1px solid #e1e1e1;height:40px;line-height: 40px;color: #333;}
.balance .balance-1 strong { display: block; float: left;  }
.balance  span { display: block; float: right;color: #db3652;}
.balance ul li{width: 96%;border-bottom:1px solid #e1e1e1;padding: 0 2%;color: #333;line-height: 35px;text-indent: 20px;background: url(/assets/images/ico_25.png) no-repeat left center;background-size: 16px 16px;background-position: 2%;}
.balance ul li.selected{background: url(/assets/images/ico_25a.png) no-repeat left center;background-size: 17px 17px;background-position: 2%;}
.confirmPay{position: fixed;bottom: 0;width: 92%;margin:4%;text-align: center;line-height: 37px;color: #fff;background: #db3652;border-radius: 5px;font-size: 16px;overflow:hidden;overflow-x;overflow-y;}

/*pc页面*/
@media only screen and (min-width: 1000px) {
			body {width: 480px;margin: 0 auto;}
			.header,.footer,.btnD,.btnF,.shopa,.dldt2 dt,.shopa2,.btnC,.confirmPay,.goPay{left: initial; width: 480px;}
			.confirmPay,.goPay{margin: 0 0 10px;}
}
@media only screen and (min-width:320px ) {
	.luckyNum{padding: 5px 0 0 10px;}
	.wait-pay-result h2{width: 42%;}
}	
@media only screen and (min-width: 360px) {
	.luckyNum{padding: 12px 0 0 10px;margin-bottom: 5px;}
	.wait-pay-result h2{width: 36%;}
}
@media only screen and (min-width: 375px) {
	.luckyNum{padding: 12px 0 0 10px;margin-bottom: 5px;}
	.wait-pay-result h2{width: 36%;}
}
@media only screen and (min-width: 400px) {
	.luckyNum{padding: 12px 0 0 10px;margin-bottom: 8px;}
	.wait-pay-result h2{width: 34%;}
}
@media only screen and (min-width: 480px) {
	.luckyNum{padding: 15px 0 0 10px;margin-bottom: 15px;}
	.wait-pay-result h2{width: 32%;}
}
@media only screen and (min-width: 320px) and (max-width: 329px) {
	.goodsList-mt{margin-right: 40px;}
}
